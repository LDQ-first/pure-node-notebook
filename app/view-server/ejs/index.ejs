<pre>
    <code>
         $.ajax({
            url: '/user.action',
            method: 'get',
            success: function(arr){
                console.log(arr);
                var liStr = arr.map(function(ele){
                    return '<li>'+ ele +'</li>'
                }).join('');
                $('#root').html(liStr);

            },
            error: function(error){
                console.log(error);
            }
        })
